    219              OS_ENTER_CRITICAL();
    220              len   = OS_StrCopy(pname, pmem->OSMemName);  /* Copy name from OS_MEM                              */
    221              OS_EXIT_CRITICAL();
    222              *perr = OS_ERR_NONE;
